  DESCRIPTION = "Subleger Trial Balance Report Definitions"


  BEGIN VSET_TABLE
    OWNER = "ORACLE12.1.3"
    LAST_UPDATE_DATE = "2009/04/28 00:00:00"
    TABLE_APPLICATION_SHORT_NAME = "XLA"
    APPLICATION_TABLE_NAME = "XLA_TB_DEFINITIONS_VL"
    SUMMARY_ALLOWED_FLAG = "N"
    VALUE_COLUMN_NAME = "NAME"
    VALUE_COLUMN_TYPE = "V"
    VALUE_COLUMN_SIZE = "80"
    ID_COLUMN_NAME = "DEFINITION_CODE"
    ID_COLUMN_TYPE = "V"
    ID_COLUMN_SIZE = "30"
    MEANING_COLUMN_NAME = "DESCRIPTION"
    MEANING_COLUMN_TYPE = "V"
    MEANING_COLUMN_SIZE = "80"
    ENABLED_COLUMN_NAME = "'Y'"
    COMPILED_ATTRIBUTE_COLUMN_NAME = "NULL"
    HIERARCHY_LEVEL_COLUMN_NAME = "NULL"
    START_DATE_COLUMN_NAME = "to_date(null)"
    END_DATE_COLUMN_NAME = "to_date(null)"
    SUMMARY_COLUMN_NAME = "'N'"
    ADDITIONAL_WHERE_CLAUSE =
 "WHERE enabled_flag = 'Y'\n\
  AND ( \n\
    definition_code IN (\n\
    SELECT xtd.definition_code\n\
    FROM xla_tb_defn_je_sources xtd\n\
              ,xla_subledgers xs\n\
     WHERE xtd.je_source_name = xs.je_source_Name\n\
     AND xs.application_id = :$FLEX$.XLA_SRS_SUBLEDGERS\n\
          )\n\
  OR DECODE( :$FLEX$.XLA_SRS_SUBLEDGERS,101,'GL','OTHERS') = 'GL'\n\
  )\n\
  AND \n\
  (\n\
    (NVL(FND_PROFILE.VALUE('XLA_USE_LEDGER_SECURITY'), 'N') = 'N') \n\
    OR \n\
    (\n\
     NVL(FND_PROFILE.VALUE('XLA_USE_LEDGER_SECURITY'), 'N') = 'Y' \n\
     AND EXISTS\n\
     ( \n\
         SELECT 1 \n\
         FROM GL_ACCESS_SETS GAS, \n\
                   GL_ACCESS_SET_ASSIGNMENTS ASA \n\
         WHERE GAS.ACCESS_SET_ID = ASA.ACCESS_SET_ID \n\
                       AND ASA.LEDGER_ID = XLA_TB_DEFINITIONS_VL.LEDGER_ID \n\
                       AND (GAS.ACCESS_SET_ID = \n\
                           NVL(FND_PROFILE.VALUE('GL_ACCESS_SET_ID'), '-1') OR \n\
                           GAS.ACCESS_SET_ID = \n\
  NVL(FND_PROFILE.VALUE('XLA_GL_SECONDARY_ACCESS_SET_ID'), \n\
                                                    '-1')))))"
