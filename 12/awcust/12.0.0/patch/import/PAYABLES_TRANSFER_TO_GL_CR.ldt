  BEGIN VSET_TABLE
    OWNER = "ORACLE12.1.3"
    LAST_UPDATE_DATE = "2005/06/10 00:00:00"
    TABLE_APPLICATION_SHORT_NAME = "SQLAP"
    APPLICATION_TABLE_NAME = "fnd_lookups FL"
    SUMMARY_ALLOWED_FLAG = "N"
    VALUE_COLUMN_NAME = "Meaning"
    VALUE_COLUMN_TYPE = "V"
    VALUE_COLUMN_SIZE = "80"
    ID_COLUMN_NAME = "Lookup_code"
    ID_COLUMN_TYPE = "V"
    ID_COLUMN_SIZE = "30"
    ENABLED_COLUMN_NAME = "'Y'"
    COMPILED_ATTRIBUTE_COLUMN_NAME = "NULL"
    HIERARCHY_LEVEL_COLUMN_NAME = "NULL"
    START_DATE_COLUMN_NAME = "to_date(null)"
    END_DATE_COLUMN_NAME = "to_date(null)"
    SUMMARY_COLUMN_NAME = "'N'"
    ADDITIONAL_WHERE_CLAUSE =
 "WHERE FL.lookup_type = 'YES_NO'\n\
  AND (\n\
  (\n\
  (FL.lookup_code = 'Y'\n\
  and 'Y' = (select nvl(asp.include_reporting_sob,'N') from ap_system_parameters asp)\n\
  and 'N' = (select nvl(asp.gl_transfer_allow_override,'N') from ap_system_parameters asp )\n\
  and ap_gl_transfer_pkg.ap_mc_check(:$FLEX$.set_of_books_id) <> 'R'\n\
  )\n\
  or \n\
  ((FL.lookup_code = 'N' or FL.lookup_code = 'Y')\n\
  and 'Y' = (select nvl(asp.gl_transfer_allow_override,'N') from ap_system_parameters asp)\n\
  and ap_gl_transfer_pkg.ap_mc_check(:$FLEX$.set_of_books_id) <> 'R'\n\
  )  \n\
  or \n\
  (FL.lookup_code = 'N' \n\
  and 'N' = (select nvl(asp.include_reporting_sob,'N') from ap_system_parameters asp)\n\
  and 'N' = (select nvl(asp.gl_transfer_allow_override,'N') from ap_system_parameters asp)\n\
  )\n\
  )\n\
  OR \n\
  (FL.lookup_code = 'N' \n\
  and ap_gl_transfer_pkg.ap_mc_check(:$FLEX$.set_of_books_id) = 'R'\n\
  )\n\
  )"
