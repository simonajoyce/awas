<?xml version = '1.0' encoding = 'UTF-8'?>
<page xmlns:jrad="http://xmlns.oracle.com/jrad" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:ui="http://xmlns.oracle.com/uix/ui" version="10.1.3_1312" xml:lang="en-US" xmlns:user="http://xmlns.oracle.com/jrad/user" xmlns="http://xmlns.oracle.com/jrad" file-version="$Header$">
   <content>
      <oa:pageLayout id="pageLayoutRN" controllerClass="oracle.apps.awcust.person.webui.UpdateEmployeeNameCO" amDefName="oracle.apps.awcust.person.server.EmployeeDetailsAM" windowTitle="Last Name Update Window">
         <ui:corporateBranding>
            <oa:image id="corporateBrandingImage" source="/OA_MEDIA/FNDSSCORP.gif"/>
         </ui:corporateBranding>
         <ui:contents>
            <oa:defaultSingleColumn id="empUpdateRN">
               <ui:contents>
                  <oa:messageStyledText id="FirstName" viewName="EmployeeDetailsVO1" viewAttr="FirstName" prompt="First Name"/>
                  <oa:messageTextInput id="LastName" viewName="EmployeeDetailsVO1" viewAttr="LastName" prompt="Last Name" columns="50" maximumLength="50"/>
                  <oa:messageStyledText id="EmployeeId" viewName="EmployeeDetailsVO1" viewAttr="PersonId" rendered="false"/>
               </ui:contents>
            </oa:defaultSingleColumn>
            <oa:pageButtonBar id="PageButtonRN">
               <ui:contents>
                  <oa:submitButton id="Apply" use="/oracle/apps/fnd/attributesets/Buttons/Apply"/>
                  <oa:submitButton id="Cancel" use="/oracle/apps/fnd/attributesets/Buttons/Cancel" serverUnvalidated="true" unvalidated="true"/>
               </ui:contents>
            </oa:pageButtonBar>
         </ui:contents>
      </oa:pageLayout>
   </content>
</page>
