<customization xmlns="http://xmlns.oracle.com/jrad" version="9.0.5.4.81_481" xml:lang="en-US" customizes="/oracle/apps/cs/csz/incident/webui/CaseCreationPG" package="/oracle/apps/cs/csz/incident/webui/customizations/function/CSZ_CASE_CR_FN" file-version="$Header: CaseCreationPG.xml 120.4 2006/01/04 16:37 ujagarla noship $">
