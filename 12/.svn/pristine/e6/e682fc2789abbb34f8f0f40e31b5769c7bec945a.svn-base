<?xml version = '1.0' encoding = 'UTF-8'?>
<customization customizes="/oracle/apps/per/selfservice/personalinformation/webui/BasicDetNamUpdRN" developerMode="false" xmlns="http://xmlns.oracle.com/jrad" xmlns:ui="http://xmlns.oracle.com/uix/ui" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:user="http://xmlns.oracle.com/user" file-version="$Header: BasicDetNamUpdRN.xml 120.0 2005/06/03 02:30:29 appldev noship $" version="9.0.5.4.64_464" xml:lang="en-US" package="/oracle/apps/per/selfservice/personalinformation/webui/customizations/localization/RU">
