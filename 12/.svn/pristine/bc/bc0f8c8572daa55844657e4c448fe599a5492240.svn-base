<?xml version = '1.0' encoding = 'UTF-8'?>
<customization customizes="/oracle/apps/fnd/wf/worklist/webui/AdvancWorklistRG" developerMode="true" xmlns="http://xmlns.oracle.com/jrad" xmlns:ui="http://xmlns.oracle.com/uix/ui" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:user="http://xmlns.oracle.com/jrad/user" file-version="$Header$" version="9.0.3.8.9_1050" xml:lang="en-US">
   <modifications>
      <modify element="NtfWorklist" blockSize="10"/>
      <modify element="NtfSelector" initSortSeq="none" required="no" readOnly="false"/>
      <modify element="NtfSubject" initSortSeq="none" tipMessageName="" required="no"/>
      <modify element="NtfDue" prompt="Due - Fn Level" initSortSeq="none" tipMessageName="" required="no"/>
      <modify element="NtfPriority" initSortSeq="none" tipMessageName="" required="no"/>
      <modify element="NtfSent" initSortSeq="none" tipMessageName="" required="no"/>
      <modify element="NtfStatus" initSortSeq="none" tipMessageName="" required="no"/>
      <modify element="NtfType" initSortSeq="none" tipMessageName="" required="no"/>
      <modify element="NtfFromUser" initSortSeq="none" tipMessageName="" required="no"/>
      <move element="NtfReassign" after="NtfOpen"/>
      <move element="NtfClose" after="NtfReassign"/>
      <move element="NtfSent" after="NtfSubject"/>
      <move element="NtfDue" after="NtfSent"/>
      <move element="NtfPriority" after="NtfDue"/>
      <move element="NtfFromUser" after="NtfPriority"/>
